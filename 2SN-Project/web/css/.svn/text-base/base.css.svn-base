body { 
    background-color: #D2D1D0;
    color: #444;
    font: 13px Tahoma, Geneva, sans-serif;
    margin: 0 auto;
    text-align: left;
}

div{
    display: block;
}
header{
    background-color: #232323;
    border-bottom: 1px solid #151515;
    display: block;
    min-width: 984px;
    position: relative;
    color: #FFF;
}
header #container{
    height: 64px;
    position: relative;
}

#container{
    margin: auto;
    width: 984px;
    margin-bottom: 10px;
}
#container-nav{
    margin-left:-50px;
    width: 40px;
    position: fixed;
}

header a#top {
width: 175px;
}
/*  NAV Menu  */
nav[role="navigation"] {
    background-color: #333;
    border-top: 1px solid #383838;
    border-bottom: 1px solid #444;
    -moz-box-shadow: 0 2px 0 0 #262626, 0 -1px 0 0 #171717;
    -webkit-box-shadow: 0 2px 0 0 #262626, 0 -1px 0 0 #171717;
    -o-box-shadow: 0 2px 0 0 #262626, 0 -1px 0 0 #171717;
    box-shadow: 0 2px 0 0 #262626, 0 -1px 0 0 #171717;
    display: block;
    margin-bottom: 1px;
    min-width: 984px;
    position: relative;
    z-index: 3100;
    color: #FFF;
    
}

.clearfix:after {
    clear: both;
    content: ".";
    display: block;
    height: 0;
    visibility: hidden;
    font-size: 0;
}
nav[role="navigation"] ul, .info-nav {
    margin: 0 auto;
    list-style: none;
    width: 994px;
}
nav[role="navigation"] > ul {
    position: relative;
    left: -5px;
}

.topbar-ul{
    margin:0;
    margin-left: 510px;
}
.topbar{
    margin-left:10px;
}
.menu-main{
   /* margin-left: 20px;
    height: 20px;
    margin-top: 5px;
    margin-bottom: 5px;*/
    height:20px;
    margin-bottom:7px;
    margin-left:-35px;
    margin-top:7px;
}

.quick-nav {
    margin-left:  -44.5px;
    margin-top: 0px;
    height: 32px;
    width: 35px;
    list-style: none;
}
nav[role="navigation"] ul, .info-nav {
    list-style: none;
}

nav[role="navigation"] ul li, .info-nav li {
  display:inline-block;
  float:left;
  font-size:12px;
  line-height:18px;
  position:relative;
  z-index:5000;
}
/*  CONTENT  */
#content {
    padding-top: 20px;
    border-top: 1px solid white;
    margin: 0 auto;
    min-height: 540px;
    padding: 20px 0 30px;
}

div.shadowed {
    border: 1px solid #BBB;
    border-image: initial;
}

div.shadowed div.inner-boundary {
    margin: -2px 0px 0px -2px;
    border: 1px solid #BBB;
    background-color: #F4F4F4;
    background-image: url('images/white_line.png');
    background-repeat: repeat-x;
    border-image: initial;
}



div.shadowed div.inner-boundary div.inner-border {
    border: 1px solid #FEFEFE;
    background-color: #F4F4F4;
    border-image: initial;
}

